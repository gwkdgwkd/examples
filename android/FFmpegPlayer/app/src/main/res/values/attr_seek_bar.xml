<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MySeekBar">
        <attr name="seek_unreached_color" format="color" />
        <attr name="seek_reached_color" format="color" />
        <attr name="seek_reached_bar_height" format="dimension" />
        <attr name="seek_unreached_bar_height" format="dimension" />
        <attr name="seek_text_size" format="dimension" />
        <attr name="seek_text_color" format="color" />
        <attr name="seek_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>
</resources>